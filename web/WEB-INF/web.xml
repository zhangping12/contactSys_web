<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <servlet>
        <servlet-name>AddContactServlet</servlet-name>
        <servlet-class>gz.itcast.contactSys_web.servlet.AddContactServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteContactServlet</servlet-name>
        <servlet-class>gz.itcast.contactSys_web.servlet.DeleteContactServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListContactServlet</servlet-name>
        <servlet-class>gz.itcast.contactSys_web.servlet.ListContactServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>QueryContactServlet</servlet-name>
        <servlet-class>gz.itcast.contactSys_web.servlet.QueryContactServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateContactServlet</servlet-name>
        <servlet-class>gz.itcast.contactSys_web.servlet.UpdateContactServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddContactServlet</servlet-name>
        <url-pattern>/AddContactServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteContactServlet</servlet-name>
        <url-pattern>/DeleteContactServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListContactServlet</servlet-name>
        <url-pattern>/ListContactServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>QueryContactServlet</servlet-name>
        <url-pattern>/QueryContactServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateContactServlet</servlet-name>
        <url-pattern>/UpdateContactServlet</url-pattern>
    </servlet-mapping>
</web-app>